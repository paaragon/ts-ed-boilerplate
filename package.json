{
  "name": "@orange/reportsts",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "npm run tsc",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'node_modules/**/*' --exec ts-node src/index.ts",
    "start": "ts-node src/index.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/interfaces/**/*.ts",
      "src/index.ts",
      "src/Server.ts",
      "src/typeorm-config.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "lines": "80",
    "functions": "80",
    "branches": "80",
    "statements": "80"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/common": "5.0.7",
    "@tsed/core": "5.65.7",
    "@tsed/di": "5.0.7",
    "@tsed/swagger": "^5.0.7",
    "@tsed/testing": "5.0.7",
    "@tsed/typeorm": "^5.2.2",
    "@types/async": "^2.4.1",
    "@types/swagger-schema-official": "^2.0.15",
    "@types/uuid": "^3.4.4",
    "async": "^2.6.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.1",
    "concurrently": "^3.5.1",
    "config": "^3.0.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.5.0",
    "method-override": "^2.3.10",
    "node-uuid": "^1.4.8",
    "oracledb": "^3.1.2",
    "ts-json-properties": "1.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.11.1",
    "@types/http-proxy": "^1.16.2",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/method-override": "0.0.31",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.3.0",
    "@types/request-promise": "^4.1.41",
    "@types/sinon": "^5.0.7",
    "@types/sinon-chai": "^2.7.32",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.1",
    "rimraf": "^2.6.2",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.1.0",
    "source-map-support": "^0.5.11",
    "sqlite3": "^4.0.6",
    "supertest": "^3.4.2",
    "ts-node": "^6.0.5",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}
